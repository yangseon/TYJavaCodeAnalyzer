<?xml version="1.0" encoding="UTF-8"?>
<FileFilter>
	<Filter name = "default">
		<ExcludedPathPatterns>
			<Path>\.svn\</Path>
			<Path>\.cvs\</Path>
			<Path>\.idea\</Path>
			<Path>\bin\</Path>
			<Path>\lib\</Path>
		</ExcludedPathPatterns>
		<Extentions>
			<Extention encoding="UTF-8">xml</Extention>
			<Extention encoding="UTF-8">js</Extention>
			<Extention encoding="UTF-8">jsp</Extention>
			<Extention encoding="UTF-8">html</Extention>
			<Extention encoding="UTF-8">htm</Extention>
			<Extention encoding="UTF-8">jelly</Extention>
			<Extention encoding="UTF-8">java</Extention>
		</Extentions>
	</Filter>
	
	<Filter name = "productivity">
		<ExcludedPathPatterns>
			<Path>\.svn\</Path>
			<Path>\.cvs\</Path>
		</ExcludedPathPatterns>
		<Extentions>
			<Extention>xml</Extention>
			<Extention>js</Extention>
			<Extention>jsp</Extention>
			<Extention>html</Extention>
			<Extention>htm</Extention>
			<Extention>jelly</Extention>
			<Extention>java</Extention>
		</Extentions>
	</Filter>
	
	<Filter name = "quality">
		<ExcludedPathPatterns>
			<Path>\.svn\</Path>
			<Path>\.cvs\</Path>
			<Path>\test\</Path>			
		</ExcludedPathPatterns>
		<Extentions>
			<Extention>xml</Extention>
			<Extention>js</Extention>
			<Extention>jsp</Extention>
			<Extention>html</Extention>
			<Extention>htm</Extention>
			<Extention>jelly</Extention>
			<Extention>java</Extention>
		</Extentions>
	</Filter>
</FileFilter>	
